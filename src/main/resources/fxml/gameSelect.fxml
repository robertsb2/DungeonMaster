<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<AnchorPane id="window" prefHeight="700.0" prefWidth="900.0" stylesheets="@../css/gameSelect.css"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.SaveSelectController">
   <children>
      <ListView fx:id="saveFiles" layoutX="50.0" layoutY="79.0" prefHeight="350.0" prefWidth="800.0" onMouseClicked="#activateButtons"/>
      <Button layoutX="103.0" layoutY="550.0" mnemonicParsing="false" text="New Game" onMouseClicked="#newGame">
         <font>
            <Font size="36.0" />
         </font></Button>
      <Button fx:id="load" layoutX="418.0" layoutY="550.0" mnemonicParsing="false" text="Load" onMouseClicked="#load">
         <font>
            <Font size="36.0" />
         </font></Button>
      <Button fx:id="delete" layoutX="652.0" layoutY="550.0" mnemonicParsing="false" text="Delete" onMouseClicked="#delete">
         <font>
            <Font size="36.0" />
         </font></Button>
      <Label layoutX="50.0" layoutY="25.0" text="Saved Games">
         <font>
            <Font size="30.0" />
         </font></Label>
   </children>
</AnchorPane>
